{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:https": "next dev --experimental-https",
    "build": "next build",
    "build:open-next": "open-next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "fmt": "prettier --write \"**/*.{ts,tsx,mdx,json}\" --cache",
    "fmt:check": "prettier --check \"**/*.{ts,tsx,mdx,json}\" --cache",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run lib/db/migrate.ts",
    "db:drop": "drizzle-kit drop",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit introspect",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "clean": "rm -rf .next",
    "cleani": "rm -rf .next && rm -rf node_modules && bun i",
    "ui": "bunx shadcn-ui@latest",
    "sst": "sst",
    "prepare": "husky"
  },
  "dependencies": {
